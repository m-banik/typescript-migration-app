{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@Common/*": ["./common/*"],
      "@Components/*": ["./components/*"],
      "@Models/*": ["./models/*"],
      "@MoviesModel/*": ["./models/movies/*"],
      "@Utilities/*": ["./utilities/*"]
    }
  }
}
